$("div#people-list").html("<%= escape_javascript(render (@viewMode == :thumbs ? 'people_thumbs' : 'people_list'))%>");
<% if @viewMode == :thumbs %>
	$("#thumbs-view-button").addClass('active');
	$("#list-view-button").removeClass('active');
<% else %>
	$("#thumbs-view-button").removeClass('active');
	$("#list-view-button").addClass('active');
<% end %>