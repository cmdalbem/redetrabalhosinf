$("div#projects-table").html("<%= escape_javascript(render (@viewMode == :list ? 'projects_list' : 'projects_thumbs'))%>");
<% if @viewMode == :thumbs %>
	$("#thumbs-view").addClass('active');
	$("#list-view").removeClass('active');
<% else %>
	$("#thumbs-view").removeClass('active');
	$("#list-view").addClass('active');
<% end %>